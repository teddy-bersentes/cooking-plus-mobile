{
	"cli": {
		"version": ">= 3.8.1",
		"appVersionSource": "remote"
	},
	"build": {
		"development": {
			"distribution": "internal",
			"channel": "development",
			"developmentClient": true,
			"ios": {
				"buildConfiguration": "Debug",
				"resourceClass": "m-medium"
			}
		},
		"staging": {
			"distribution": "internal",
			"channel": "staging",
			"ios": {
				"resourceClass": "m-medium"
			}
		},
		"production": {
			"autoIncrement": true,
			"channel": "production",
			"ios": {
				"resourceClass": "m-medium"
			}
		}
	},
	"submit": {
		"production": {}
	}
}